{
    "name": "@doptools/plugin-workspace",
    "version": "0.0.1-0",
    "scope": "@doptools",
    "schematics": "./lib/schematics.json",
    "bugs": "https://github.com/doptools/cli/issues",
    "homepage": "https://github.com/doptools/cli",
    "repository": "https://github.com/doptools/cli",
    "scripts": {
        "clean": "rimraf .tmp/dist",
        "build": "run-s build:*",
        "build:schema": "gulp buildSchema",
        "build:ts": "tsc -p tsconfig.plugin.json",
        "build:copy": "gulp copyFiles",
        "version": "echo TODO: update version.json",
        "prepack": "run-s prepack:*",
        "prepack:clean": "yarn clean",
        "prepack:build": "yarn build",
        "prepack:manifest": "oclif-dev manifest",
        "prepack:readme": "oclif-dev readme",
        "postpack": "run-s postpack:*",
        "postpack:manifest": "rm -f oclif.manifest.json",
        "xversion": "oclif-dev readme && git add README.md",
        "start": "run-p start:*",
        "start:schema": "gulp",
        "start:ts": "tsc -p tsconfig.plugin.json -w"
    },
    "oclif": {
        "commands": "./lib/commands",
        "bin": "dops",
        "devPlugins": [
            "@oclif/plugin-help"
        ]
    },
    "files": [
        "/dist",
        "/npm-shrinkwrap.json",
        "/oclif.manifest.json",
        "/yarn.lock"
    ],
    "dependencies": {
        "@angular-devkit/core": "^11.2.3",
        "@angular-devkit/schematics": "^11.2.3",
        "hosted-git-info": "^3.0.8"
    },
    "devDependencies": {
        "@angular-devkit/schematics-cli": "^0.1102.3",
        "@doptools/cli-core": "^0.1.3-dev.1",
        "@oclif/dev-cli": "1.26.0",
        "@types/gulp": "^4.0.8",
        "@types/node": "^14.14.31",
        "glob": "^7.1.6",
        "gulp": "^4.0.2",
        "json-schema-to-typescript": "^10.1.3",
        "npm-run-all": "^4.1.5",
        "rimraf": "^3.0.2",
        "tslint": "^6.1.3",
        "tslint-config-prettier": "^1.18.0",
        "tslint-config-standard": "^9.0.0",
        "typescript": "^4.2.2"
    }
}
